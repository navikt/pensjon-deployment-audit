{
  "name": "pensjon-deployment-audit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "tsx scripts/migrate.ts && react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "check": "npm run lint && npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "node-pg-migrate up -m app/db/migrations",
    "db:migrate:down": "node-pg-migrate down -m app/db/migrations",
    "db:migrate:create": "node-pg-migrate create -m app/db/migrations",
    "db:init": "tsx scripts/init-db.ts",
    "test:nais-discovery": "tsx scripts/test-nais.ts discovery",
    "test:nais-fetch": "tsx scripts/test-nais.ts fetch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "^5.2.5",
    "@navikt/aksel-icons": "^8.1.0",
    "@navikt/ds-css": "^8.1.0",
    "@navikt/ds-react": "^8.1.0",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@react-pdf/renderer": "^4.3.2",
    "@react-router/node": "7.12.0",
    "@react-router/serve": "7.12.0",
    "@slack/bolt": "^4.6.0",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "isbot": "^5.1.31",
    "jose": "^6.1.3",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.17.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@react-router/dev": "7.12.0",
    "@types/node": "^25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "lefthook": "^2.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18"
  }
}
